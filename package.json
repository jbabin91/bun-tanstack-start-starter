{
  "name": "bun-tanstack-start-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun vite build",
    "dev": "bun --bun vite dev --port 3000",
    "format": "bun --bun prettier -uwl --cache .",
    "format:check": "bun --bun prettier -uc --cache .",
    "lint": "bun --bun eslint . --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "bun --bun eslint . --fix --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "serve": "bun --bun vite preview",
    "start": "bun run .output/server/index.mjs",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.4",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.136.5",
    "@tanstack/react-router-devtools": "^1.136.5",
    "@tanstack/react-router-ssr-query": "^1.136.5",
    "@tanstack/react-start": "^1.136.5",
    "@tanstack/router-plugin": "^1.136.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "nitro": "latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.10",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-plus": "^1.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "jsdom": "^27.2.0",
    "lefthook": "^2.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
